generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model UserSession {
  userId    String   @id @map("user_id")
  sessionId String   @map("session_id")
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  @@map("user_sessions")
}
